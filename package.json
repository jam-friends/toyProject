{
  "packageManager": "yarn@3.4.1",
  "version": "0.1.0",
  "private": true,
  "workspaces": {
    "packages": [
      "package/**"
    ]
  },
  "scripts": {
    "@start/shearcher": "yarn workspace package/study_searcher",
    "lint": "eslint ./package --ext .ts,.tsx,.js,.jsx --cache",
    "test:all": "yarn workspaces foreach --parallel run test"
  }
}
